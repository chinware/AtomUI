<UserControl
    mc:Ignorable="d"
    x:Class="AtomUI.Demo.Desktop.ShowCase.CalendarShowCase"
    xmlns="https://github.com/avaloniaui"
    xmlns:atom="https://atomui.net"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:showcase="clr-namespace:AtomUI.Demo.Desktop.ShowCase"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <showcase:ShowCasePanel>
        <showcase:ShowCaseItem Description="The simplest usage for Calendar." Title="Basic">
            <atom:Calendar />
        </showcase:ShowCaseItem>

        <showcase:ShowCaseItem Description="The simplest usage for Calendar." Title="Range Calendar">
            <atom:RangeCalendar />
        </showcase:ShowCaseItem>
    </showcase:ShowCasePanel>
</UserControl>