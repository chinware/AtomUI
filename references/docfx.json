{
    "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
    "metadata": [
        {
            "src": [
                {
                    "src": "../src",
                    "files": [
                        "AtomUI.Base/AtomUI.Base.csproj",
                        "AtomUI.Controls/AtomUI.Controls.csproj",
                        "AtomUI.Theme/AtomUI.Theme.csproj"
                    ]
                }
            ],
            "dest": "../../output/references/api"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "**/*.{md,yml}"
                ],
                "src": "../../output/references"
            },
            {
                "files": [
                    "**/*.{md,yml}"
                ]
            }
        ],
        "resource": [
        ],
        "output": "../../output/references/site",
        "template": [
            "default",
            "modern"
        ],
        "globalMetadata": {
            "_appName": "qinware",
            "_appTitle": "qinware",
            "_enableSearch": true,
            "pdf": false
        }
    }
}