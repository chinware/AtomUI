{
    "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
    "metadata": [
        {
            "src": [
                {
                    "src": "../src",
                    "files": [
                        "AtomUI.Base/AtomUI.Base.csproj",
                        "AtomUI.Generator/AtomUI.Generator.csproj",
                        "AtomUI.Theme/AtomUI.Theme.csproj",
                        "AtomUI.IconPkg/AtomUI.IconPkg.csproj",
                        "AtomUI.IconPkg.AntDesign/AtomUI.IconPkg.AntDesign.csproj",
                        "AtomUI.Controls/AtomUI.Desktop.Controls.csproj",
                        "AtomUI.ReactiveUI/AtomUI.ReactiveUI.csproj",
                        "AtomUI.Desktop.Controls.ColorPicker/AtomUI.Desktop.Controls.ColorPicker.csproj",
                        "AtomUI.Desktop.Controls.DataGrid/AtomUI.Desktop.Controls.DataGrid.csproj"
                    ]
                }
            ],
            "output":"../../output/references",
            "properties": {
                "Configuration": "Debug"
            }
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "**/*.{md,yml}"
                ],
                "src": "../../output/references"
            },
            {
                "files": [
                    "**/*.{md,yml}"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "output": "../../output/references/site",
        "template": [
            "default",
            "modern",
            "lumi-apidoc-template"
        ],
        "globalMetadata": {
            "_appName": "qinware",
            "_appTitle": "qinware",
            "_enableSearch": true,
            "pdf": false
        }
    }
}