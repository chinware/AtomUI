<Project>
    <PropertyGroup>
        <PackageOutputPath>$(MSBuildThisFileDirectory)..\output\Nuget\</PackageOutputPath>
        <OutputPathWithoutFramework>$(MSBuildThisFileDirectory)..\output\bin\$(Configuration)\</OutputPathWithoutFramework>
        <OutputPath>$(OutputPathWithoutFramework)\$(TargetFramework)\</OutputPath>
        <IntermediateOutputPathWithoutFramework>$(MSBuildThisFileDirectory)..\output\$(MSBuildProjectName)\obj\$(Configuration)\</IntermediateOutputPathWithoutFramework>
        <IntermediateOutputPath>$(IntermediateOutputPathWithoutFramework)$(TargetFramework)\</IntermediateOutputPath>
    </PropertyGroup>
</Project>