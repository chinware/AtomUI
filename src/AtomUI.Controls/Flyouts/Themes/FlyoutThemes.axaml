<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:themes="using:AtomUI.Controls.Themes"
                    xmlns:atom="https://atomui.net"
                    x:ClassModifier="internal">
    <themes:MenuFlyoutPresenterTheme x:Key="{x:Type atom:MenuFlyoutPresenter}" TargetType="atom:MenuFlyoutPresenter"/>
    <themes:TreeViewFlyoutPresenterTheme x:Key="{x:Type atom:TreeViewFlyoutPresenter}" TargetType="atom:TreeViewFlyoutPresenter"/>
    <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="FlyoutHostTheme.axaml"/>
    </ResourceDictionary.MergedDictionaries>

</ResourceDictionary>