<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    x:ClassModifier="internal">
    <ControlTheme x:Key="{x:Type atom:AvatarGroup}" TargetType="atom:AvatarGroup">
        <Setter Property="GroupSpace" Value="{DynamicResource {x:Static atom:AvatarTokenKey.GroupSpace}}" />
        <Setter Property="GroupOverlapping" Value="{DynamicResource {x:Static atom:AvatarTokenKey.GroupOverlapping}}" />
        <Setter Property="FoldInfoAvatarForeground"
                Value="{DynamicResource {x:Static atom:AvatarTokenKey.AvatarColor}}" />
        <Setter Property="FoldInfoAvatarBackground" Value="{DynamicResource {x:Static atom:AvatarTokenKey.AvatarBg}}" />
        <Setter Property="BorderThickness" Value="{DynamicResource {x:Static atom:SharedTokenKey.BorderThickness}}" />
    </ControlTheme>
</ResourceDictionary>