<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    x:ClassModifier="internal">
    <ControlTheme x:Key="{x:Type atom:PaginationNav}" TargetType="atom:PaginationNav">
        <Setter Property="Template">
            <ControlTemplate>
                <Border Name="{x:Static atom:PaginationNavThemeConstants.FramePart}"
                        ClipToBounds="True"
                        CornerRadius="{TemplateBinding CornerRadius}"
                        Padding="{TemplateBinding Padding}">
                    <atom:NavItemsPresenter Name="{x:Static atom:PaginationNavThemeConstants.ItemsPresenterPart}" />
                </Border>
            </ControlTemplate>
        </Setter>
    </ControlTheme>
</ResourceDictionary>