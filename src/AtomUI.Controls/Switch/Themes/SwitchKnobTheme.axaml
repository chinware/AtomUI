<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    x:ClassModifier="internal">
    <ControlTheme x:Key="{x:Type atom:SwitchKnob}" TargetType="atom:SwitchKnob">
        <Setter Property="LoadingBgOpacity" Value="{DynamicResource {x:Static atom:ToggleSwitchTokenKey.SwitchDisabledOpacity}}" />
        <Setter Property="LoadingAnimationDuration" Value="{DynamicResource {x:Static atom:ToggleSwitchTokenKey.LoadingAnimationDuration}}" />
    </ControlTheme>
</ResourceDictionary>