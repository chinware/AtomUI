<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    xmlns:themes="using:AtomUI.Controls.Themes">
    <ControlTheme x:Key="{x:Static atom:NavMenuThemeConstants.VerticalNavMenuItemThemeId}" TargetType="atom:NavMenuItem">
        <ControlTheme.BasedOn>
            <themes:NavMenuItemTheme TargetType="atom:NavMenuItem" />
        </ControlTheme.BasedOn>
    </ControlTheme>
</ResourceDictionary>