<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    xmlns:themes="using:AtomUI.Controls.Themes"
    x:ClassModifier="internal">
    <ControlTheme x:Key="{x:Type atom:GradientColorSlider}" TargetType="atom:GradientColorSlider">
        <ControlTheme.BasedOn>
            <themes:AbstractColorSliderTheme TargetType="atom:GradientColorSlider" />
        </ControlTheme.BasedOn>
    </ControlTheme>
</ResourceDictionary>