<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    xmlns:themes="using:AtomUI.Controls.Themes">
    <themes:DefaultButtonTheme x:Key="{x:Static atom:DefaultButtonTheme.ID}" TargetType="atom:Button"/>
    <themes:PrimaryButtonTheme x:Key="{x:Static atom:PrimaryButtonTheme.ID}" TargetType="atom:Button"/>
    <themes:LinkButtonTheme x:Key="{x:Static atom:LinkButtonTheme.ID}" TargetType="atom:Button"/>
    <themes:TextButtonTheme x:Key="{x:Static atom:TextButtonTheme.ID}" TargetType="atom:Button"/>
</ResourceDictionary>