<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    xmlns:themes="using:AtomUI.Controls.Themes">
    <themes:DefaultDropdownButtonTheme x:Key="{x:Static themes:DefaultDropdownButtonTheme.ID}" TargetType="atom:DropdownButton"/>
    <themes:PrimaryDropdownButtonTheme x:Key="{x:Static themes:PrimaryDropdownButtonTheme.ID}" TargetType="atom:DropdownButton"/>
    <themes:LinkDropdownButtonTheme x:Key="{x:Static themes:LinkDropdownButtonTheme.ID}" TargetType="atom:DropdownButton"/>
    <themes:TextDropdownButtonTheme x:Key="{x:Static themes:TextDropdownButtonTheme.ID}" TargetType="atom:DropdownButton"/>
</ResourceDictionary>