<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    xmlns:calendarView="using:AtomUI.Controls.CalendarView"
    x:ClassModifier="internal">
    <ControlTheme x:Key="{x:Type calendarView:RangeCalendar}" TargetType="calendarView:RangeCalendar">
        <ControlTheme.BasedOn>
            <atom:CalendarTheme TargetType="calendarView:RangeCalendar" />
        </ControlTheme.BasedOn>
        <Setter Property="Template">
            <ControlTemplate>
                <Border Name="{x:Static atom:DatePickerCalendarThemeConstants.FramePart}"
                        ClipToBounds="True"
                        Background="{TemplateBinding Background}"
                        Padding="{TemplateBinding Padding}"
                        MinWidth="{TemplateBinding MinWidth}"
                        MinHeight="{TemplateBinding MinHeight}">
                    <calendarView:RangeCalendarItem
                        Name="{x:Static atom:DatePickerCalendarThemeConstants.CalendarItemPart}"
                        HorizontalAlignment="Stretch"
                        IsMotionEnabled="{TemplateBinding IsMotionEnabled}" />
                </Border>
            </ControlTemplate>
        </Setter>
    </ControlTheme>
</ResourceDictionary>