<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    x:ClassModifier="internal"
                    xmlns:atom="https://atomui.net"
                    xmlns:themes="using:AtomUI.Controls.Themes">
    <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="ListTheme.axaml" />
        <ResourceInclude Source="ListDefaultViewTheme.axaml" />
        <ResourceInclude Source="ListGroupItemTheme.axaml" />
    </ResourceDictionary.MergedDictionaries>
    <themes:ListItemTheme x:Key="{x:Type atom:ListItem}" TargetType="atom:ListItem" />
</ResourceDictionary>