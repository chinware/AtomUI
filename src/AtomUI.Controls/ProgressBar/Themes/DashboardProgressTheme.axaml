<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:themes="using:AtomUI.Controls.Themes"
    xmlns:atom="https://atomui.net">
    <ControlTheme x:Key="{x:Type atom:DashboardProgress}" TargetType="atom:DashboardProgress">
        <ControlTheme.BasedOn>
            <themes:AbstractCircleProgressTheme TargetType="atom:DashboardProgress" />
        </ControlTheme.BasedOn>
    </ControlTheme>
</ResourceDictionary>