<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    xmlns:themes="using:AtomUI.Controls.Themes"
    x:ClassModifier="internal">
    <themes:AddOnDecoratedBoxTheme x:Key="{x:Type atom:AddOnDecoratedBox}" TargetType="atom:AddOnDecoratedBox"/>
    <themes:AddOnDecoratedInnerBoxTheme x:Key="{x:Type atom:AddOnDecoratedInnerBox}" TargetType="atom:AddOnDecoratedInnerBox"/>
</ResourceDictionary>