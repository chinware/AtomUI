<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    xmlns:themes="using:AtomUI.Controls.Themes"
    x:ClassModifier="internal">
    <ControlTheme x:Key="{x:Type atom:GradientColorPicker}" TargetType="atom:GradientColorPicker">
        <ControlTheme.BasedOn>
            <themes:AbstractColorPickerTheme TargetType="atom:GradientColorPicker" />
        </ControlTheme.BasedOn>
    </ControlTheme>
</ResourceDictionary>