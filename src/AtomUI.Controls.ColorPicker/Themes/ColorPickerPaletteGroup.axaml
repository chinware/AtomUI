<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    x:ClassModifier="internal">
    <ControlTheme x:Key="{x:Type atom:ColorPickerPaletteGroup}" TargetType="atom:ColorPickerPaletteGroup">
        <Setter Property="Template">
            <ControlTemplate>
                <atom:Collapse ItemsSource="{TemplateBinding PaletteGroup}">
                    <atom:Collapse.ItemTemplate>
                        <DataTemplate>
                            <atom:CollapseItem Header="{Binding Label}" Content="Fuck"/>
                        </DataTemplate>
                    </atom:Collapse.ItemTemplate>
                </atom:Collapse>
            </ControlTemplate>
        </Setter>
    </ControlTheme>
</ResourceDictionary>