<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:atom="https://atomui.net"
                    xmlns:themes="using:AtomUI.Controls.Themes"
                    x:ClassModifier="internal">
    <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="SearchEditDecoratedBoxTheme.axaml" />
        <ResourceInclude Source="TextAreaDecoratedBoxTheme.axaml" />
        <ResourceInclude Source="SearchEditTheme.axaml" />
        <ResourceInclude Source="ResizeHandleTheme.axaml" />
    </ResourceDictionary.MergedDictionaries>
    <themes:TextBoxTheme x:Key="{x:Type atom:TextBox}" TargetType="atom:TextBox"/>
    <themes:LineEditTheme x:Key="{x:Type atom:LineEdit}" TargetType="atom:LineEdit"/>
    <themes:TextAreaTheme x:Key="{x:Type atom:TextArea}" TargetType="atom:TextArea"/>
    <themes:SearchButtonTheme x:Key="{x:Type atom:SearchButton}" TargetType="atom:SearchButton"/>
</ResourceDictionary>