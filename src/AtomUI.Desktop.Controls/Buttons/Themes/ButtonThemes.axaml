<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:atom="https://atomui.net"
                    xmlns:themes="using:AtomUI.Desktop.Controls.Themes"
                    x:ClassModifier="internal">
    <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="Button/ButtonTheme.axaml"/>
        <ResourceInclude Source="DropdownButton/DropdownButtonThemes.axaml"/>
        <ResourceInclude Source="SplitButtonTheme.axaml"/>
        <ResourceInclude Source="HyperLinkButtonTheme.axaml"/>
    </ResourceDictionary.MergedDictionaries>
    <themes:IconButtonTheme x:Key="{x:Type atom:IconButton}" TargetType="atom:IconButton"/>
    <themes:ToggleIconButtonTheme x:Key="{x:Type atom:ToggleIconButton}" TargetType="atom:ToggleIconButton"/>
</ResourceDictionary>